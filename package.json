  "private": true,
  "browserslist": [
    "> 0.20%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "yarn hook:pre-commit"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix",
      "stylelint",
      "jest --bail --find-related-tests"
    ],
    "*.{json,md,mdx,yml}": [
      "prettylint --fix"
    ]
  }
