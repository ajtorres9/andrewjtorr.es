{
  "name": "andrewjtorr.es",
  "version": "0.1.0",
  "description": "Personal website of Andrew Torres",
  "keywords": [
    "personal-website",
    "website"
  ],
  "homepage": "https://github.com/ajtorres9/andrewjtorr.es#readme",
  "bugs": "https://github.com/ajtorres9/andrewjtorr.es/issues",
  "license": "MIT",
  "author": "Andrew Torres <andrew.jonathan.torres@gmail.com> (https://andrewjtorr.es)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ajtorres9/andrewjtorr.es.git"
  },
  "scripts": {
    "build": "webpack --config config/webpack.config.js",
    "build:stats": "yarn build --analyze --release",
    "lint": "yarn lint:files && yarn lint:scripts && yarn lint:styles && yarn lint:types",
    "lint:files": "prettylint --ignore-path .gitignore \"**/*.{json,md,yml}\"",
    "lint:scripts": "eslint --cache --ignore-path .gitignore --ignore-pattern \"!**/.*\" \"**/*.{js,ts,tsx}\"",
    "lint:styles": "stylelint --cache --ignore-path .gitignore --ignore-pattern \"!**/.*\" \"**/*.{ts,tsx}\"",
    "lint:types": "tsc --noEmit",
    "start": "webpack-dev-server --config config/webpack.config.js",
    "test": "yarn test:units",
    "test:units": "jest"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.5.5",
    "@loadable/component": "^5.10.2",
    "@reach/router": "^1.2.1",
    "core-js": "^3.2.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-is": "^16.8.6",
    "styled-components": "5.0.0-beta.8"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-react-constant-elements": "^7.5.0",
    "@babel/plugin-transform-react-inline-elements": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@semantic-release/commit-analyzer": "^6.3.0",
    "@semantic-release/exec": "^3.3.5",
    "@semantic-release/git": "^7.0.16",
    "@semantic-release/github": "^5.4.2",
    "@semantic-release/release-notes-generator": "^7.3.0",
    "@svgr/webpack": "^4.3.2",
    "@testing-library/jest-dom": "^4.0.0",
    "@testing-library/react": "^9.1.2",
    "@types/jest": "^24.0.17",
    "@types/loadable__component": "^5.10.0",
    "@types/reach__router": "^1.2.4",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.8.5",
    "@types/styled-components": "^4.1.18",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "babel-plugin-polished": "^1.1.0",
    "babel-plugin-styled-components": "^1.10.6",
    "codecov": "^3.5.0",
    "cz-conventional-changelog": "^3.0.2",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-standard": "^13.0.1",
    "eslint-config-standard-react": "^8.0.0",
    "eslint-import-resolver-typescript": "^1.1.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.15.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-unicorn": "^10.0.0",
    "file-loader": "^4.2.0",
    "husky": "^3.0.3",
    "jest": "^24.8.0",
    "jest-circus": "^24.8.0",
    "jest-config": "^24.8.0",
    "jest-styled-components": "7.0.0-beta.1",
    "lint-staged": "^9.2.1",
    "polished": "^3.4.1",
    "prettier": "^1.18.2",
    "prettylint": "^1.0.0",
    "semantic-release": "^15.13.19",
    "stylelint": "^10.1.0",
    "stylelint-config-prettier": "^5.2.0",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-order": "^3.0.1",
    "stylelint-processor-styled-components": "^1.8.0",
    "terser-webpack-plugin": "^1.4.1",
    "typescript": "~3.5.3",
    "url-loader": "^2.1.0",
    "webpack": "^4.39.2",
    "webpack-bundle-analyzer": "^3.4.1",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2"
  },
  "engines": {
    "node": ">=10.16.3",
    "yarn": ">=1.16.0"
  },
  "private": true,
  "browserslist": [
    "> 0.20%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix",
      "stylelint",
      "jest --bail --findRelatedTests",
      "git add"
    ],
    "*.{json,md,yml}": [
      "prettylint --fix",
      "git add"
    ]
  }
}
